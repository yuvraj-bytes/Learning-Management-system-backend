{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "projectSettings": {
    "framework": "nestjs",
    "buildSettings": {
      "outputDirectory": "dist",
      "buildCommand": "npm run build"
    }
  }
}